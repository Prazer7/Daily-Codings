package ch03;

public class Ch03Ex11 {

public static void main(String[] args) {

int iVar1 = 5;

if(iVar1 > 10) { // if문에는 '{}' (영역 표시)가 붙음

System.out.printlf("10보다 크다");

} 

// iVar1이 10보다 작으므로 출력하지 X (결과 값 X)

int iVar2 = 5;

if (iVar2 < 10) {

System.out.printlf("10보다 작다");

} 

// iVar2가 10보다 작으므로 출력 (결과 값 O)


} 


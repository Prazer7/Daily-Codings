		System.out.print(123);
		System.out.println(456); 
		System.out.println(789); 
		System.out.println(0.123); 
		System.out.println('A'); 
		System.out.println("ABC"); 
		System.out.println(true); 
	}
}
=================================================변수의 선언과 출력=======================================================

package ch01;
public class Ch02Ex02 {

	public static void main(String[] args) {
  int data1; // 변수 선언
  data1 = 1; // 변수 초기화
  
  int data2;
  data2 = 2;
  
  system.out.println(data1+data2);
  system.out.println(data1-data2);
  system.out.println(data1*data2);
  system.out.println(data1/data2);
  
  data2 = 5;  // data2의 값을 변경
  system.out.println(data1+data2);
  
  int data3 = 3;   //  변수의 선언과 초기화 동시 작성 가능
  int a=1, b=2, c=3   //  여러개의 값을 동시에 작성 가능
  
  }
============================================변수의 종류와 Wrapper Class===================================================
package ch02;
public class Ch02Ex04 {

	public static void main(String[] args) {

		byte byteVar = 127; // 메모리에 정수 8bit의 공간 확보
		short shortVar = 30000; // 메모리에 정수 16bit의 공간 확보
		int intVar = 2100000000; // 메모리에 정수 32bit의 공간 확보 (정수 변수의 기본형)
		long longVar = 2200000000L; // 메모리에 정수 64bit의 공간 확보 (숫자 끝에 'L'을 입력)
		
		float floatVar = 0.123456789F; //  메모리에 정수 32bit의 공간 확보 (숫자 끝에 'F'를 입력)
		System.out.println(floatVar); //  
		double doubleVar = 0.123456789D; // 메모리에 정수 64bit의 공간 확보 (숫자 끝에 'D' 입력은 선택사항)
		System.out.println(doubleVar);
		
		boolean boolVar = false;   //  true or false
		System.out.println(boolVar);
		System.out.println(longVar > intVar); //  true
		
		char charVar = 'A';   //  문자 출력
		System.out.println(charVar);
		
		System.out.println(Byte.MIN_VALUE + "~" + Byte.MAX_VALUE);
		System.out.println(Short.MIN_VALUE + "~" + Short.MAX_VALUE);
		System.out.println(Integer.MIN_VALUE + "~" + Integer.MAX_VALUE);
		System.out.println(Long.MIN_VALUE + "~" + Long.MAX_VALUE);

	}

}
=============================================Data type casting========================================================
package ch02;
public class Ch02Ex08 {

	public static void main(String[] args) {
		
		byte byteVar1 = 1, byteVar2 = 2;
		int intVar1 = byteVar1; 
		// byte (8bit)을  int (32bit)로 대입 => 에러 X (Promotion)
		
		System.out.println(intVar1);
		
		// byte byteVar3 = intVar1; 
		// ㄴbyte (32bit)을 int (8bit)로 대입 => 에러 O (Demotion)
		
		byte byteVar3 = (byte) intVar1;
		System.out.println(byteVar3);
		
		byte byteVar4 = (byte) (byteVar1 + byteVar2); // Demotion
		System.out.println(byteVar4);
		
		
		int intVar2 = 129;
		byte byteVar5 = (byte) intVar2;
		System.out.println(byteVar5);
		
		//  byte는 127까지 표현할 수 있음
		
	} 
} 
=============================================Data type casting2========================================================
package ch02;



public class Ch02Ex10 {



public static void main(String[] args) {

double doubleVar1 = 3.14;    //   64bit

long longVar1 = doubleVar1;  //   실수와 정수간의 충돌

int intVar1 = (int) doubleVar1; //   Demotion (강제적 형태 변환)

System.out.println(intVar1);  

}

}
=============================================Data type casting3========================================================
package ch02;

public class Ch02Ex10 {

public static void main(String[] args) {


float floatVar1 = 3.14F;

double doubleVar1 = floatVar1;  // Promotion (자동적 형태 변환)

System.out.println(doubleVar1);


double doubleVar2 = 3.14;

float floatVar2 = (float) doubleVar2;  // Demotion (강제적 형태 변환)

System.out.println(floatVar2);


}

}
=========================================문자형 변수 (char) / 아스키 코드 변환=============================================
package ch02;

public class Ch02Ex13 {

public static void main(String[] args) {

char chVar1;

chVar1 = 'A';   /// A <= 65 (아스키 코드 'A' 그대로 표현)

System.out.println(​chVar1);

char chVar2 = 36;    // 36 => $ (아스키 코드 '$'로 변환)  

System.out.println(chVar2);


int iVar1;

iVar1 = 'H';   

// H => 72 

(int는 'H'를 표현할 수 없기 때문에 아스키코드 H에 해당하는 정수 72로 자동 변환)

System.out.println(iVar1); 

ㄴ문자형 변수 (char)는 정수형 변수 (int)와 호환 가능 (아스키 코드표 근거)


int iVar2 = 88;   

char chVar3;

chVar3 = (char) iVar2;   // 강제적으로 아스키 코드 'X'로 변경 (Demotion)

System.out.println(chVar3);

}

}

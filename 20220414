===================================================Scanner 입력===================================================
package ch03;

import java.util.Scanner;

// Scanner - jdk가 제공하는 입력기.

// Scanner scan = new Scanner(System.in);

// System.in : 키보드에서 입력 받는 값들이 저장되는 입력 버퍼(메모리).

// 키보드 입력하면 입력 버퍼에 중간 저장. -> Scanner를 통해 중간 저장된 값을 읽어 온다.


public class Ch03Ex12 {

public static void main(String[] args) {

Scanner scan = new Scanner(System.in);

// Scanner에 빨간 밑줄 -> [Ctrl]+[Space Bar] -> 'Scanner.util' 선택 -> import java.util.Scanner; 추가됨


// 'new'를 이용한 객체 생성

// System.in: 입력 버퍼 (키보드에서 입력된 값들이 보관되는 곳) 


System.out.println("0에서 21억 미만의 정수를 입력하세요.");

int iVar = scan.nextInt();

//scan.nextInt() : 정수 가져오기

System.out.println("your input : " + iVar);

System.out.println("실수를 입력하세요.");

double dVar = scan.nextDouble();

//scan.nextDouble() : 실수 가져오기

System.out.println("your input : " + dVar);

System.out.println("단어를 입력하세요.");

String str1 ​= scan.next();

//scan.next() : 단어 가져오기


System.out.println("your input : " + str1);

scan.nextLine(); 

// nextLine() : 문장 가져오기 (버려진 엔터값을 받아서 사라지는 문장을 추가)

System.out.println("문장을 입력하세요.");

String str2 = scan.nextLine();

System.out.println("your input : " + str2);


scan.close(); // 반드시 close.

} 


} 

===============================================If문을 활용한 가위바위보 게임=============================================
package ch03;

import java.util.Scanner;

public class Ch03Ex14 {

	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);

		//1 : 가위, 2 : 바위, 3 : 보
		System.out.print("정수 1 / 2 / 3 입력 : ");
		int yourChoice = scan.nextInt(); // 다음에 올 정수
		System.out.println("Your Choice : " + yourChoice);

		int comChoice = (int) (Math.random() * 3 + 1);
		//Math.random() : 0.0 ~ 1.0 미만
	    //Math.random() * 3 => 0.0 ~2.999999 => 1, 2, 3을 나오게 하기위해 +1 수행
		System.out.println("Computer Choice : " + comChoice);

		scan.close();
		
		if(yourChoice == 1 && comChoice == 2) {
			System.out.println("패배");
		}
		if(yourChoice == 2 && comChoice == 3) {
			System.out.println("패배");
		}
		if(yourChoice == 3 && comChoice == 1) {
			System.out.println("패배");
		}

		if(yourChoice == 1 && comChoice == 3) {
			System.out.println("승리");
		}
		if(yourChoice == 2 && comChoice == 1) {
			System.out.println("승리");
		}
		if(yourChoice == 3 && comChoice == 2) {
			System.out.println("승리");
		}

		if(yourChoice == comChoice) {
			System.out.println("비김");
		}
		

	}//main

}//class

========================================if~else if~else문을 활용한 놀이공원 티켓 구입===================================================
package ch03;

import java.util.Scanner;

/*
 * if : 분기문 : branch statement
 *  - 조건에 따라 수행하거나 수행하지 않는 코드 블럭을 만든다.
 *  - if(수행 조건) {수행 코드}
 *  - (수행 조건)이 true이면, {수행 코드}를 수행.
 *  - (수행 조건)이 false이면, {수행 코드}를 수행하지 않는다.
 */
/* if ~ else
 *  - if(수행 조건) {if 수행 코드} ~ else {else 수행 코드}
 *  - (수행 조건)이 true이면, {if 수행 코드}를 수행.
 *  - (수행 조건)이 false이면, {else 수행 코드}를 수행.
 */
/*
 * if ~ else if ~ else
 *  - if(수행 조건) {if 수행 코드}
 *    else if(else if 수행 조건) {else if 수행 코드}
 *    else {else 수행 코드}
 *  - if(수행 조건)이 true이면, {if 수행 코드}를 수행.
 *  - else if(else if 수행 조건)이 true이면, {else if 수행 코드}를 수행.
 *  - (수행 조건)도 false이고 (else if 수행 조건)도 false이면, {else 수행 코드}를 수행.
 */
//else if는 무한대로 추가 가능.
public class Ch03Ex21 {

	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);

		//놀이동산//0:없음, 1:종일권, 2:오전권, 3:오후권, 4:야간권.
		System.out.println("0:없음, 1:종일권, 2:오전권, 3:오후권, 4:야간권.");
		System.out.print("메뉴를 선택하세요 : ");
		
		int ticket = scan.nextInt();

		if(ticket == 1) {//티켓이 있다면 환영해라.
			System.out.println("놀이 동산에 오신 것을 환영합니다.");
		} else if(ticket == 2) {
			System.out.println("오후 3시에 퇴장을 안내한다.");
		} else if(ticket == 3) {
			System.out.println("오후 3시부터 입장을 안내한다.");
		} else if(ticket == 4) {
			System.out.println("오후 6시부터 입장을 안내한다.");
		} else {
			System.out.println("매표소를 안내한다.");
		}

		scan.close();//반드시 닫아야 한다. 안닫으면? 현장에서 욕먹음.

	}//main

}//class
========================================if~else if~else문을 활용한 성적 평균 구하기===================================================

package ch03;

import java.util.Scanner;

public class Ch03Ex26 {

	public static void main(String[] args) {
		
		int kor, eng, math, sum, avg;

		Scanner scan = new Scanner(System.in);
		System.out.print("국어 점수 입력 : ");
		kor = scan.nextInt();
		System.out.print("영어 점수 입력 : ");
		eng = scan.nextInt();
		System.out.print("수학 점수 입력 : ");
		math = scan.nextInt();
		scan.close();

		sum = kor + eng + math;
		avg = sum / 3;

		System.out.println("국어 : " + kor);
		System.out.println("영어 : " + eng);
		System.out.println("수학 : " + math);
		System.out.println("합계 : " + sum);
		System.out.println("평균 : " + avg);

		//avg = 99; 이라도, 조건에 부합하는 최초의 if(수행 조건) 하나를 수행하고 나머지 if문은 무시된다.
		if(avg >= 90) {
			System.out.println("Grade A.");
		} else if (avg >= 80) {
			System.out.println("Grade B.");
		} else if (avg >= 70) {
			System.out.println("Grade C.");
		} else if (avg >= 60) {
			System.out.println("Grade D.");
		} else {
			System.out.println("Grade F.");
		}

	}//main

}//class

============================================switch~case문을 활용한 이동 수단 입력========================================================
package ch03;

import java.util.Scanner;

public class Ch03Ex30 {
	// switch~case문: byte, short, int, long, char, String만 switch case 가능.
	public static void main(String[] args) {
		
		System.out.println("이동 수단을 입력하세요 : ");
		System.out.println("1:비행기, 2:기차, 3:버스");
		Scanner scan = new Scanner(System.in);
		int howMove = scan.nextInt();
		scan.close();
		
		switch(howMove) { // Scanner로 정수를 입력 받아서 switch~case문 수행
		case 1 : System.out.println("부산까지 비행기로 1시간 소요 예상."); break;
		case 2 : System.out.println("부산까지 기차로 3시간 소요 예상."); break;
		case 3 : System.out.println("부산까지 버스로 5시간 소요 예상."); break;
		default : System.out.println("입력 값이 올바르지 않습니다."); break;
		// default : switch~case문에서 나머지 모든 값을 의미 (=else)
		
		}

	} // main

} // class

=====================================================while문을 활용한 구구단 만들기=========================================================
package ch03;

public class Ch03Ex39 {

	public static void main(String[] args) {
		int dan = 2; // 초기식
		while(dan <= 9) { // 조건식
			System.out.println("===" + dan + "단===");
			
			int gop = 1; // 초기식
			while(gop <= 9) { //조건식
				System.out.printf("%d * %d = %d\t", dan, gop, dan * gop);
				gop++; // 증감식
			}
		
			dan++; // 증감식
			System.out.println();
		}
	} // main

} // close

=====================================================while문을 활용한 학년별 반 출력=========================================================
package ch03;

public class Ch03Ex37 {

	public static void main(String[] args) {
		// while문의 중첩 : 초기식, 비교식, 증감식의 위치를 기억하자.
		int hak = 1; // 학년 (초기식)
		while(hak <= 3) { // 비교식
			System.out.println(hak + "학년");
			
			//학년별 10반까지 출력해라.
			int ban = 1; // 반
		    while(ban <= 10) {
		    System.out.println("\t" + ban + "반");
		    ban++;
		    }
			
			hak++; // 증감식
		}

	} // main

} // class
=====================================================for문을 활용한 구구단 출력=========================================================
package ch03;

public class Ch03Ex39 {

	public static void main(String[] args) {
		int dan = 2; // 초기식
		while(dan <= 9) { // 조건식
			System.out.println("===" + dan + "단===");
			
			int gop = 1; // 초기식
			while(gop <= 9) { //조건식
				System.out.printf("%d * %d = %d\t", dan, gop, dan * gop);
				gop++; // 증감식
			}
			
			dan++; // 증감식
			System.out.println(); // 줄바꿈
		}
	} // main

} // close
=====================================================for문을 활용한 학년별 반 출력=========================================================
package ch03;
/*
 * for : 반복문 (Loop Statement)
 *  - 초기식, 조건식, 증감식을 한 곳으로 모아 놓은 형태
 *  - for(초기식; 조건식; 증감식) {수행 코드}
 */

public class Ch03Ex40 {
	

	public static void main(String[] args) {
		//0에서 10까지 출력.
		for(int endPoint = 0; endPoint <= 10; endPoint++) {
			System.out.print(endPoint + "\t");
			
		}
		System.out.println(); // 줄바꿈
		//10에서 0까지 출력.
			for(int endPoint = 10; endPoint >= 0; endPoint--) {
				System.out.print(endPoint + "\t");
		} // for문
	}// main	
		
} // class

=========================================if~else~if~else문을 활용한 계산기 만들기==========================================================
package ch03;

import java.util.Scanner;

public class Ch03Ex32 {

	public static void main(String[] args) {
	
		int fNum, sNum, res = 0; // 첫번째 숫자, 두번째 숫자, 결과값 0으로 초기화
		String giho; // 부호 선언
		Scanner scan = new Scanner(System.in);

		System.out.print("정수 입력 : ");
		fNum = scan.nextInt();

		System.out.print("+ - * / 중 하나의 기호 입력 : ");
		giho = scan.next();

		System.out.print("정수 입력 : ");
		sNum = scan.nextInt();

		scan.close();

		if( giho.equals("+") ) {  // 문자열 비교문 equals (정수: ==)
			System.out.println(fNum + sNum);
		} else if( giho.equals("-") ) {
			System.out.println(fNum - sNum);
		} else if( giho.equals("*") ) {
			System.out.println(fNum * sNum);
		} else if( giho.equals("/") ) {
			System.out.println(fNum / sNum);
		} else {
			System.out.println("입력된 값이 올바르지 않습니다.");
			System.out.println("계산기를 종료 합니다.");
			System.exit(0); // 프로그램 종료 표시
		}
		System.out.printf("%d %s %d = %d\n", fNum, giho, sNum, res); // 수식 검사

	}//main

}//class

=========================================================== break를 통한 while/for문 중단시키기 ===========================================================

package ch03;

/*
 * break : stop을 명령할 때.
 *  - 프로그램의 흐름을 종료 시킨다.
 *  - 가장 가까운 반복문만 종료.
 */

public class Ch03Ex52 {

	public static void main(String[] args) {
		
		for(int dan = 2; dan <= 9; dan++) {
			for(int gop = 1; gop <= 9; gop++) {
				System.out.printf("%d * %d = %d\t", dan, gop, dan * gop);
				if(gop == 5) break;
			}
			System.out.println(); // 줄바꿈
		
		}
		
	} // main

} // class
=======================================================continue를 통한 while/for문 생략 ==============================================================
package ch03;

/*
 * continue
 *  - 프로그램의 흐름을 건너 뛴다. (생략)
 *  - 가장 가까운 반복문만 종료.
 */

public class Ch03Ex53 {

	public static void main(String[] args) {
		
		for(int dan = 2; dan <= 9; dan++) {
			for(int gop = 1; gop <= 9; gop++) {
				if(gop == 5) continue; // 생략하고 싶은 특정 조건이 있을 때
				// 아래 printf 문을 생략하기 때문에 먼저 선언해야 한다.
				System.out.printf("%d * %d = %d\t", dan, gop, dan * gop);
				
			}
			System.out.println(); // 줄바꿈
		}
		
	} // main

} // class

================================================for문을 활용한 홀수/짝수/배수 출력==============================================
package ch03;

public class Ch03Ex56 {

	public static void main(String[] args) {
		//1에서 100까지 홀수 출력
		for(int i = 1; i <= 100; i++) {
			if(i % 2 == 1) 	
			System.out.printf("%d\t", i);
			 // = System.out.println(i);
		}//for
		System.out.println();
		//1에서 100까지 짝수 출력
		for(int i = 1; i <= 100; i++) {
			if(i % 2 == 0)
			System.out.printf("%d\t", i);
			// = System.out.println(i);
		}//for

		System.out.println();
		//0에서 200까지 13의 배수 출력
		for(int i = 0; i <= 200; i++) {
			if(i % 13 == 0) 
		    System.out.printf("%d\t", i);
			// = System.out.println(i);
		}//for
	}//main
}//class

===================================================if문을 활용한 양수/음수 판별=========================================================
package ch03;

import java.util.Scanner;

public class Ch03Ex57 {

	public static void main(String[] args) {
		//한 개의 정수를 입력받아
		//양수(positive integer)인지 음수(negative number)인지 출력하는 작업을 반복하다가
		//0이 입력되면 종료하는 프로그램을 작성하시오.
		Scanner scan = new Scanner(System.in);
		
		int userInput = 0; // 반복문 초기식
		while(true) {
			System.out.print("정수를 입력하세요 : "); 
			// 해당 문구가 반복되어야 하므로 while문 안에 있어야 한다.
			userInput = scan.nextInt();
			if(userInput == 0) break; // 프로그램 종료 (break를 걸어줬으므로 증감식은 생략된다)
			else if (userInput > 0) 
				System.out.println("positive integer"); // 양수 출력
			else if (userInput < 0) 
				System.out.println("negative number"); // 음수 출력
		}// while문 안에 if문이 있어야 한다.

		scan.close(); // 모든 입력이 끝나는 지점에 작성해준다.
	}//main

}//class

==================================================배열 (Array) 기본 개념과 수행=====================================================
package ch041;

public class Ch041Ex03 {

	public static void main(String[] args) {
		// 배열은 길이 변경이 불가능 하다.
		int [] iArr1 = new int [3]; // iArr1이라는 배열에 3칸짜리 공간 생성
		iArr1[0] = 7; // 7대입
		iArr1[1] = 8;
		iArr1[2] = 9;
		System.out.println(iArr1[2]);
		
		iArr1 = new int [5]; // iArr1 배열 재선언 (수정) 
		iArr1[0] = 1;
		iArr1[1] = 2;
		iArr1[2] = 3;
		iArr1[3] = 4;
		iArr1[4] = 5;
		// 기존에 7,8,9가 담긴 3칸짜리 배열은 사라지고,
		// 0으로 초기화 된 5칸짜리 배열이 새로 연결된다.
		System.out.println(iArr1[2]);

	} // main
} // class

==================================================배열 (Array)을 이용한 3의 배수 출력=====================================================
package ch041;

public class Ch041Ex051 {

	public static void main(String[] args) {
		
		int [] iArr = {99, 88, 77, 66, 55};
		for (int i = 0; i < iArr.length; i++) { 
			if(iArr[i] % 3 ==0) { // iArr의 i번지로 접근 
				System.out.println("3으로 나눈 나머지가 0인 수 : " + iArr[i]);
			}
		}

	} // main

} // class

==================================================배열 (Array)을 이용한 로또 번호 출력=====================================================
package ch041;

public class Ch041Ex053 {

	public static void main(String[] args) { 
		int lotto[] = new int [6]; // 번호 생성 
		for(int i=0; i<6; i++) { 
			lotto[i] = (int)(Math.random() * 45) + 1;  
			
			for(int j=0; j<i; j++) { // 중복 번호 제거
				//ㄴ i가 0일 때는 수행하지 X
				if(lotto[i] == lotto[j]) { 
					i--; // 중복된 값을 제거하기 위해 i값을 다시 내려준다 (1->0)
					break; 
					} 
				} 
			} 
		
		System.out.print("로또 번호: "); // 번호 출력 
		for(int i=0; i<6; i++) { 
			System.out.print(lotto[i] + " "); 
		} 
		} 
	}

==================================================배열 (Array)의 오름차순 정렬========================================================
package ch041;

import java.util.Arrays;

public class Ch041Ex055 {

	public static void main(String[] args) {

		int [] iArr2 = {34, 16, 20, 11, 7, 14};
		System.out.println(Arrays.toString(iArr2));
		
		Arrays.sort(iArr2); // 오름차순으로 정렬
		System.out.println(Arrays.toString(iArr2));
	
	
	}//main

}///class
=======================================================2차원 배열=================================================================

package ch041;

public class Ch041Ex06 {

	public static void main(String[] args) {
		//이차원 배열의 선언
		int [][] multiArr1; int []multiArr2[]; int multiArr3 [][];

		//이차원 배열의 생성 방법
		multiArr1 = new int [2][3]; // 아파트2동 각3층.
		multiArr1[0][0] = 1;
		multiArr1[0][1] = 2;
		multiArr1[0][2] = 3;
		multiArr1[1][0] = 4;
		multiArr1[1][1] = 5;
		multiArr1[1][2] = 6; // 총 6개의 층수
		System.out.println(multiArr1[1][1]);

		
		int [][] multiArr4 = new int [3][3];
		int [][] multiArr5 = { {1,2}, {3,4,5}, {6,7,8,9} };
		
		int [][] multiArr6 = new int[3][];
		multiArr6[0] = new int [2];
		multiArr6[1] = new int [3];
		multiArr6[2] = new int [4];

	}//main

}//class
=======================================================객체 지향 프로그래밍=================================================================
package ch042;

public class Ch042Ex01 {

	public static void main(String[] args) {
		
		Ch042Ex01 test = new Ch042Ex01();
		test.print();
		test.plus(777, 8);
		boolean imsi = test.compareStringLength("hi", "hello");
		System.out.println(imsi);
	}//main

	public void print() {
		
		System.out.println("출력 메소드");
	}//print

	public void plus(int iVar1, int iVar2) {
		
		System.out.println(iVar1 + iVar2);
	}//plus

	public boolean compareStringLength(String str1, String str2) {
		if(str1.length() == str2.length()) return true;
		else return false;
	}//compareStringLength

}//class

=======================================================String class=================================================================

package ch042;
class Ch042Saram {

	String firstName;
	String lastName;
	String socialSequrityNumber;
	String phoneNumber;//데이터적 요소

	void sayHello() {
		System.out.println("안녕하세요. " + firstName + lastName + " 입니다.");
	}//행위적 요소
}//class
public class Ch042Ex02 {
	public static void main(String[] args) {
		//String class 하나로 여러 개의 문자열 객체를 만들 수 있다.
		String str1 = new String("hi");
		String str2 = new String("hello");
		//new : class를 메모리에 올리라는 표현(명령어)
		// . : ~의 내부에 존재하는
		
		//Ch042Saram class 하나로 여러 명의 사람 객체를 만들 수 있다.
		Ch042Saram saram1 = new Ch042Saram();
		saram1.firstName = "바";
		saram1.lastName = "다";
		saram1.sayHello();
		Ch042Saram saram2 = new Ch042Saram();
		saram2.firstName = "하";
		saram2.lastName = "늘";
		saram2.sayHello();
		//Ch042Saram class를 설계도로, 메모리에 Ch042Saram 객체를 만든다.
	}//main
}//class
===============================================데이터 모델링을 이용한 게시판 만들기====================================================
package ch042;

import java.util.Scanner;

class Ch042Board {

	String title;
	String writer;
	String contents;
	String date;
	String category;

	void print() {
		System.out.println("구분 : " + category);
		System.out.println("제목 : " + title);
		System.out.println("작성자 : " + writer);
		System.out.println("작성일 : " + date);
		System.out.println("내용 : " + contents);
	}

}//class

public class Ch042Ex03 {

	public static void main(String[] args) {

		Ch042Board [] bArr = new Ch042Board[10];
		
		Scanner scan = new Scanner(System.in);
		int menuNo = 0, bangBunHo = 0;

		while(true) {
			System.out.print("0:종료 / 1:작성 / 2:출력, 선택 : ");
			menuNo = scan.nextInt();
			scan.nextLine();//nextInt()엡서 입력된 엔터 값을 갖고 없어지는 표현.

			if(menuNo == 0) {
				scan.close();
				break;
			} else if (menuNo == 1) {
				if(bangBunHo == 10) {
					System.out.println("게시글이 꽉 찼습니다. 관리자에게 문의 하세요.");
					continue;
				}
				bArr[bangBunHo] = new Ch042Board();
				System.out.print("제목 : ");
				bArr[bangBunHo].title = scan.nextLine();
				System.out.print("작성자 : ");
				bArr[bangBunHo].writer = scan.nextLine();
				System.out.print("내용 : ");
				bArr[bangBunHo].contents = scan.nextLine();
				System.out.print("날짜 : ");
				bArr[bangBunHo].date = scan.nextLine();
				System.out.print("공지 / QnA / 자유, 구분 : ");
				bArr[bangBunHo].category = scan.nextLine();
				bangBunHo++;
			} else if (menuNo == 2) {
				for (int i = 0; i < bangBunHo; i++) {
					bArr[i].print();
				}//for
			} else {
				System.out.println("wrong input. try again.");
			}

		}//while

	}//main

}//class
===============================================지역 변수====================================================
package ch042;
public class Ch042Ex05 {

	public static void main(String[] args) {
		
		String localSrt1 = "지역 변수 1";
		{
			// String localSrt1 = "지역 변수"; => Error (같은 이름은 선언할 수 X)
			String localSrt2 = "지역 변수 2";
		}
		    String localSrt2 = "지역 변수 2"; 
		    // 범위 (중괄호) 밖에 있기 때문에 같은 이름으로 선언 가능.
		    
		    for(int idx = 0; idx <= 10; idx++) {
		    	System.out.println("idx도 지역 변수");
		    
		    	//int idx = 5; => Error (같은 이름은 선언할 수 X)
		    } // for
		    
		    for(int idx = 0; idx <= 10; idx++) {
		    	System.out.println("idx도 지역 변수 2"); 
		    	// 범위 (중괄호) 밖에 있기 때문에 같은 이름으로 선언 가능.
		    } // for
		
	} // main

} // class
===============================================멤버 변수====================================================
package ch042;

class Ch042Dummy {
	String memVar = "멤버 변수";
	void method1() {
		String loVar = "지역 변수";
		
	} // method1	
} // class

public class Ch042Ex06 {

	public static void main(String[] args) {
		Ch042Dummy dummy = new Ch042Dummy();
		// 클래스 호출 -> 메모리 적재 -> garbage collector

		dummy.method1();
		// 메소드 호출 -> 메모리 적재 -> 실행 종료 후 삭제
	} // main

} // class
==================================================지역 변수와 멤버 변수의 차이점====================================================
package ch042;

class Ch042Dummy2 {
	
	String memberVar = "멤버 변수";
	
	void print() {
		String memberVar = "지역 변수";
		System.out.println("memberVar : " + memberVar);
		// 멤버 변수와 지역 변수의 이름이 같을 때, 지역 변수를 우선 적용.
		
		System.out.println("this.memberVar :"  + this.memberVar);
		// this. 사용 시 멤버 변수 출력 가능
		
				
		/*
		 * this.memberVar
		 * - . : ~의 안에 있다 : a.b -> a안에 b 있다
		 * - this 안에 memberVar라는 의미
		 * - this : 현재의 instance
		 */	
		
		
		} // print
} // class

public class Ch042Ex07 {

	public static void main(String[] args) {
		
		Ch042Dummy2 dummy = new Ch042Dummy2(); // instance
		dummy.print();
		
		Ch042Dummy2 dummy2 = new Ch042Dummy2(); // instance
		dummy2.memberVar = "더미2 인스턴스";
		dummy2.print();

	} // main

} // class


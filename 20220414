=============================================equals (문자열 비교)==========================================
package ch02;

public class Ch02Ex17 {

public static void main(String[] args) {


int iVar1 = 777;

int iVar2 = 777;

System.out.println("primitive : " + (iVar1 == iVar2) );   //  '==' 같다 (논리형 기호)

String str1 = new String("hello");

String str2 = new String("hello");

System.out.println("new String : " + (str1 == str2) );

// String이라는 새로운 공간에 각각 저장되기 때문에 같지 않음 (false)

String str3 = "hello";

String str4 = "hello";

System.out.println("Another String : " + (str3 == str4) );


System.out.println("equals: " + str1.equals(str2));   //  equals : 문자열 비교

System.out.println("equals: " + str1.equals(str3));

String str5 = "Hello";

System.out.println("The other String : " + (str3 == str5​) );

System.out.println("equals :" + str1.equals(str5));

System.out.println("equalsIgnoreCase :" + str1.equalsIgnoreCase(str5));

// equalsIgnoreCase : 대소문자 무시하고 문자열 비교

}

}
=========================================concat (문자열 이어 붙이기)====================================
public class Ch02Ex15 {


public static void main(String[] args) {

String str1= "서울시";

String str2= "마포구";

String str3= "대흥동";


System.out.println(str1.concat(" ").concat(str2).concat(" ").concat(str3));

// '.concat()'을 이용해서 문자열 붙이기

System.out.println(str1 + " " + str2 + " " + str3); 

//  '+' 를 이용해서 문자열 붙이기 


} 

} 
===========================================문자열의 길이/위치값 검색=========================================
package ch02;

public class Ch02Ex16 {


public static void main(String[] args) {


String str = "nice to meet you, too.";

System.out.println(str.length());

// length() : 공백을 포함한 문자의 갯수 출력 (1부터 세움)

System.out.println(str.indexOf('.'));

System.out.println(str.indexOf("too"));

// indexOf('찾고자 하는 문자 or 문자열')

ㄴ 왼쪽에서 오른쪽으로 검색 시 가장 첫번째로 오는 '.'의 위치값 출력 (0부터 세움)


System.out.println(str.lastOfIndex('e'));

// lastOfIndex('찾고자 하는 문자 or 문자열')

ㄴ오른쪽에서 왼쪽으로 검색 시 가장 첫번째로 오는 'e'의 위치값 출력 (0부터 세움)


System.out.println(str.charAt(1));

// charAt (특정 위치값) : 왼쪽에서 오른쪽으로 검색 시 특정 위치값의 문자 출력

(0번지부터 카운트)

}

}
==============================================replace (문자열 교체)============================================
package ch02;

public class Ch02Ex18 {

public static void main(String[] args) {

String str = "nice to meet you, too.";

System.out.println(str.replace("you", "u~~"));   //  일회성 변화

// 대상 문자열.replace("바꿔질 문자 or 문자열", "바꿀 문자 or 문자열")

System.out.println("원본 : " + str);    //   본래 데이터는 변하지 않음

System.out.println(str.replace("o", "A"));

System.out.println(str.replaceAll("o", "A"));   //   replace=replaceAll

System.out.println("원본 : " + str);    //   본래 데이터는 변하지 않음


String str2 = str.replace("you", "u");  //  변경된 값 저장

System.out.println("변경된 값 : " + str2);   //   변경된 값 출력


} 

} 

====================================substring (문자열 자르기)/split (문자열 나누기)====================================
package ch02;

public class Ch02Ex19 {

public static void main(String[] args) {

String str = "nice to meet you, too.";

System.out.println(str.substring(6)); 

// str.substring (자를 문자열의 위치)

   ㄴ1번지에서 6번지까지 자름 (왼쪽에서 오른쪽으로 1번지부터 카운트)

System.out.println(str.substring(6, 14)); 

// substring(6 , 14) : 1번지~6번지까지 자른 후, 14번지 앞부터 끝까지 자름

//  6번지 포함, 14번지 미포함. (1번지부터 카운트)


System.out.println("원본 : " + str);  //  본래 데이터는 변하지 않음.


String [] sArray = str.split(" ");   

// 공백 기준으로 분리하여 sArray라는 문자 배열에 저장 

System.out.println(sArray.length)  //  공백의 수 출력


System.out.println(sArray[0]);

System.out.println(sArray[1]);

System.out.println(sArray[2]);

System.out.println(sArray[3]);

System.out.println(sArray[4]);

System.out.println("원본 : " + str);   //   본래 데이터는 변하지 않음


}

}
=============================trim (공백 자르기) /  대/소문자 변경 (toUpperCase / toLowerCase)==============================
package ch02;

public class Ch02Ex20 {

public static void main(String[] args) {

String str = "       열       공       ";

System.out.println("trim : ["+str.trim()+"]");    //   trim() : 공백 자르기

System.out.println("원본 : ["+str+"]");

//  문자열의 앞과 뒤의 공백만 잘림 (가운데 공백은 잘리지 X)

//  로그인 창의 아이디를 입력 받을 때 사용

String str2 = "HAPPY birthday To you.";

System.out.println(str2.toUpperCase());   //  toUpperCase(): 모두 대문자로 변경

System.out.println(str2.toLowerCase());  //   toLowerCase(): 모두 소문자로 변경

System.out.println("원본 : " + str2);     //    원본 불변의 법칙


} 

} 
====================================Integer.parseInt / Double.parseDouble (문자열을 숫자로 변환)=============================
package ch02;

public class Ch02Ex21 {


public static void main(String[] args) {


String str1 = "777";

System.out.println(str1 + 10);     //   문자열과 숫자는 + 연산이 되지 않는다.

int iVar1 = Integer.parseInt(str1);

System.out.println(iVar1 + 10);


String str2 = "777.77";

System.out.println(str2 + 10.0);    //    문자열과 숫자는 + 연산이 되지 않는다.

double dVar1 = Double.parseDouble(str2);

System.out.println(dVar1 + 10.0);


}

}
============================================Math Class (수학 관련 Class)===============================================
package ch02;

public class Ch02Ex22 {

public static void main(String[] args) {

System.out.println("ceil(0.1) : " + Math.ceil(0.1)); // 올림

System.out.println("floor(0.9) : " + Math.floor(0.9)); // 내림

System.out.println("round(0.4) : " + Math.round(0.4)); // 반올림

System.out.println("round(0.5) : " + Math.round(0.5)); // 반올림

System.out.println("===============================");

System.out.println("max(100, 200) : " + Math.max(100, 200));

System.out.println("min(100, 200) : " + Math.min(100, 200));

System.out.println("===============================");

System.out.println("pow(2, 5) : " + Math.pow(2, 5)); // 2의5승

System.out.println("sqrt(25) : " + Math.sqrt(25)); // 25의 제곱근

System.out.println("===============================");

System.out.println("random : " + Math.random());

System.out.println("random : " + Math.random());

System.out.println("random : " + Math.random());

// random : 무작위 수 구하기 (0.0 ~ 0.999999999999999...)

// ㄴ수행할 때마다 다른 값을 가져 온다.

System.out.println("random * 100 : " + Math.random() * 100);

// 0 ~ 100 사이의 수 구하기

System.out.println("random * 1000 : " + Math.random() * 1000);

// 0 ~ 1000 사이의 수 구하기

}

}
===========================================printf (서식을 활용한 기본 문자열 출력)=======================================
package ch02;

public class Ch02Ex23 {

public static void main(String[] args) {

System.out.printf("기본 문자열 출력 가능\n");

// \n : 줄바꿈

System.out.printf("%s\n", "기본 문자열 출력 가능");

// %s : 문자열 출력

System.out.printf("%d\n", 123);

// %d : 정수 출력.

System.out.printf("%f\n", 3.14);

// %f : 실수 출력 (소수점 6자리까지 출력)

System.out.printf("%.2f\n", 3.14);

// %.2f: 소수점 2자리까지 출력

System.out.printf("%.20f\n", 3.14);

// %와.20f:소수점 20자리까지 출력

// ㄴ자리수는 임의로 조정 가능

System.out.printf("%c\n", 'A');

// %c: 문자 출력


System.out.printf("%d + %d = %d\n", 1, 2, 3);

// 한 번에 여러 개의 값 선언 가능

System.out.printf("이름 %s, 나이 %d, 키 %.1fcm, 몸무게 %.1fkg, 혈액형 %c"

, "홍길동", 25, 180.0, 80.0, 'A');

}

}
==============================================관계 연산자(비교 연산자)===========================================
package ch03;

//관계 연산자(비교 연산자) -> 결과가 true, false.
// >, <, >=, <=, ==(같다), !=(다르다)
public class Ch03Ex02 {

	public static void main(String[] args) {

		System.out.println(10 > 100);//false
		System.out.println(10 < 100);//true
		System.out.println(10 >= 100);//false
		System.out.println(10 <= 100);//true
		System.out.println(10 == 100);//false
		System.out.println(10 != 100);//true

	}
}
=========================================논리 연산자/Short Circuit Logic===========================================
package ch03;

//논리 연산자 -> 결과 값이 true, false.
// & (and) : 그리고, 좌/우가 모두 true일때만 true.
// | (or) : 또는, 좌/우 중 하나가 true이면 true.
public class Ch03Ex03 {

	public static void main(String[] args) {

		System.out.println(true & true);//true
		System.out.println(true & false);//false
		System.out.println(false & true);//false
		System.out.println(false & false);//false

		System.out.println(true | true);//true
		System.out.println(true | false);//true
		System.out.println(false | true);//true
		System.out.println(false | false);//false

		//논리 연산의 Short Circuit Logic 1.
		//& : & 앞(좌측)의 값이 이미 거짓이면, & 뒤(우측)의 값과 상관없이 이미 거짓.
		//  -> & 뒤(우측)의 값을 검사할 필요가 없다 -> &&
		System.out.println( 1 > 10 & 10 < 100 );//false
		// & : 1 > 10 검사 + 10 < 100 검사 -> 결과 false
		System.out.println( 1 > 10 && 10 < 100 );//false
		// && : 1 > 10 검사 -> 결과 false //Short Circuit Logic

		//논리 연산의 Short Circuit Logic 2.
		// | : | 앞(좌측)의 값이 이미 참이면, | 뒤(우측)의 값과 상관없이 이미 참.
		//  -> | 뒤(우측)의 값을 검사할 필요가 없다 -> ||
		System.out.println( 10 < 100 | 1 > 10 );//true
		// | : 10 < 100 검사 + 1 > 10 검사 -> 결과 true
		System.out.println( 10 < 100 || 1 > 10 );//true
		// || : 10 < 100 검사 -> 결과 true //Short Circuit Logic

	}

}
==========================================================증감 연산자=================================================
package ch03;

public class Ch03Ex04 {

	public static void main(String[] args) {
		
		int iVar1 = 8;
		iVar1++; // 값을 하나 올림 (++)
		System.out.println("iVar1++ 1회 결과 : " + iVar1);
		
		int iVar2 = 8;
		iVar2--; // 값을 하나 내림 (--)
		System.out.println("iVar1-- 1회 결과 : " + iVar2);
		
		
		int iVar3 = 8;
		int result1 = ++iVar3 * 3;
		System.out.println("++8 * 3 : " + result1);
		//증감 연산자의 전위 (앞에 위치) : 값을 올리고 나머지를 수행.
		
		int iVar4 = 8;
		int result2 = iVar4++ * 3;
		System.out.println("8++ * 3 : " + result2);
		System.out.println("iVar4 : " + iVar4);
		//증감 연산자의 후위 (뒤에 위치) : 나머지를 수행하고 값을 올림.
		//ㄴ 8 * 3 = 24 계산 후에 8++ 수행

	}

}
==========================================================증감 연산자2=================================================
package ch03;

public class Ch03Ex05 {

	public static void main(String[] args) {
		
		int iVar1 = 8;
		int res1 = --iVar1 * 3;
		System.out.println("--8 * 3 : " + res1);
		
		int iVar2 = 8;
		int res2 = iVar2-- * 3;
		System.out.println("8-- * 3 : " + res2);
		System.out.println("iVar2 : " + iVar2);
		
	} // main

} // class

package ch03;

public class Ch03Ex06 {


public static void main(String[] args) {


System.out.println(1 > 10 ? true : false);
System.out.println(10 < 100 ? true : false);

int iVar1 = 5;

System.out.println(iVar1 >=5 ? "올림" : "내림");

iVar1 = 4;

System.out.println(iVar1 >=5 ? "올림" : "내림");

}

}
==============================================논리 부정 / 부호 연산자 / 보수====================================
package ch03; 

public class Ch03Ex07 { 

public static void main(String[] args) { 

 
System.out.println(true);

System.out.println(!true); 

System.out.println(!!true);

// ! : 논리 부정 (NOT)

System.out.println(10);

System.out.println(-10);

System.out.println( -(-10));

// +, - : 부호 연산자 

​

System.out.println( ~ 0 );

System.out.println( ~ 1 );

System.out.println( ~ 2 );

// ~ : 1의 보수 

// 1의 보수 : -128 ~ 127 -> 0의 대칭점은?? -1 

// -3 -2 -1 기준 0 1 2 

System.out.println( ~ 0 +1 );

System.out.println( ~ 1 +1 );

System.out.println( ~ 2 +1 );

//2의 보수 : 1의 보수 + 1 


}

}
============================================right / left shift 연산=============================================
package ch03; 

/* * shift 연산자 * - 일종의 비트 연산. 

* - 사용 이유 : 작은 메모리 활용. 

* 부호 64 32 16 8 4 2 1 

* 4 : 0 0 0 0 0 1 0 0 

* 4 >> 2 : 0 0 0 0 0 0 0 1 

* 4 << 2 : 0 0 0 1 0 0 0 0 

* >> : right shift : 새로운 자리는 부호(음/양)로 채움 

* << : left shift : 새로운 자리는 0으로 채움 

*/ 

public class Ch03Ex09 { 

public static void main(String[] args) { 

System.out.println( 4 >> 2 );

System.out.println( 4 << 2 );

}

}
============================================대입 연산자 / 복합 대입 연산자=============================================
package ch03;

public class Ch03Ex10 {

public static void main(String[] args) {


int iVar = 100; // 대입 연산자: 우항의 값을 좌항에 대입

iVar += 7; // iVar = iVar + 7;

iVar -= 7; // iVar = iVar - 7;

iVar *= 7; // iVar = iVar * 7;

iVar /= 7; // iVar = iVar / 7;

iVar %= 7; // iVar = iVar % 7;

iVar &= 7; // iVar = iVar & 7;

iVar |= 7; // iVar = iVar | 7;

iVar ^= 7; // iVar = iVar ^ 7;

iVar >>= 7; // iVar = iVar >> 7;

iVar <<= 7; // iVar = iVar << 7;

iVar >>>= 7; // iVar = iVar >>> 7;


} 

} 
